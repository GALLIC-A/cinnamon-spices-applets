#: applet.js:43
msgid "Open clipboard history"
msgstr "Zwischenablageverlauf öffnen"

#: applet.js:107
msgid "[SETTINGS] Settings change detected"
msgstr "[PARAMETER] Parameteränderung erkannt"

#: applet.js:115
msgid "🗑️ Clear entire history"
msgstr "🗑️ Gesamten Verlauf löschen"

#: applet.js:118
msgid "History emptied and clipboard erased!"
msgstr "Verlauf geleert und Zwischenablage gelöscht!"

#: applet.js:143
#, javascript-format
msgid "“%s” copied to clipboard."
msgstr "„%s“ in die Zwischenablage kopiert."

#: applet.js:152
msgid "Clipboard has been emptied."
msgstr "Die Zwischenablage wurde geleert."

#: applet.js:161
msgid "Clipboard cleared (it contained text)."
msgstr "Zwischenablage geleert (Textinhalt erkannt)."

#: applet.js:163
msgid "Clipboard not cleared (non-text content)."
msgstr "Zwischenablage nicht geleert (Nicht-Textinhalt)."

#: applet.js:176
#, javascript-format
msgid "An error occurred while adding content to clipboard history: %s"
msgstr "Beim Hinzufügen des Inhalts zum Verlauf der Zwischenablage ist ein Fehler aufgetreten: %s"

#: applet.js:206
#, javascript-format
msgid "New content detected: “%s”"
msgstr "Neuer Inhalt erkannt: „%s“"

#: applet.js:210
#, javascript-format
msgid "The contents of the clipboard have not changed since last %s seconds."
msgstr "Der Inhalt der Zwischenablage hat sich in den letzten %s Sekunden nicht geändert."

#: applet.js:215
msgid "Starting clipboard checking."
msgstr "Starte Überprüfung der Zwischenablage."

#: clipboard-history@axaul->metadata.json->name
msgid "Clipboard history"
msgstr "Zwischenablage-Verlauf"

#: clipboard-history@axaul->metadata.json->description
msgid "Utility to access previously copied text."
msgstr "Werkzeug, um zuvor kopierten Text anzuzeigen."

#: clipboard-history@axaul->settings-schema.json->debug_mode->description
msgid "Enable debug mode"
msgstr "Debug-Modus aktivieren"

#: clipboard-history@axaul->settings-schema.json->clipboard_history_limit->description
msgid "Maximum number of items that can be stored in the clipboard history."
msgstr "Maximale Anzahl der Elemente, die im Zwischenablage-Verlauf gespeichert werden können."

#: clipboard-history@axaul->settings-schema.json->poll_interval->description
msgid "Clipboard check interval"
msgstr "Prüfintervall der Zwischenablage"

#: clipboard-history@axaul->settings-schema.json->poll_interval->units
msgid "seconds"
msgstr "Sekunden"

#: clipboard-history@axaul->settings-schema.json->open_applet_shortcut->description
msgid "Open clipboard history with shortcut."
msgstr "Zwischenablage-Verlauf per Tastenkombination öffnen."

#: clipboard-history@axaul->settings-schema.json->open_applet_shortcut->tooltip
msgid "Save keyboard shortcuts to open the clipboard history."
msgstr "Tastenkombinationen zum Öffnen des Zwischenablage-Verlaufs speichern."
