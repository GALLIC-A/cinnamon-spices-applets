#: applet.js:43
msgid "Open clipboard history"
msgstr "Abrir histórico da área de transferência"

#: applet.js:107
msgid "[SETTINGS] Settings change detected"
msgstr "[CONFIGURAÇÕES] Alteração de configuração detectada"

#: applet.js:115
msgid "🗑️ Clear entire history"
msgstr "🗑️ Limpar todo o histórico"

#: applet.js:118
msgid "History emptied and clipboard erased!"
msgstr "Histórico esvaziado e área de transferência apagada!"

#: applet.js:143
#, javascript-format
msgid "“%s” copied to clipboard."
msgstr "“%s” copiado para a área de transferência."

#: applet.js:152
msgid "Clipboard has been emptied."
msgstr "A área de transferência foi esvaziada."

#: applet.js:161
msgid "Clipboard cleared (it contained text)."
msgstr "Área de transferência limpa (continha texto)."

#: applet.js:163
msgid "Clipboard not cleared (non-text content)."
msgstr "Área de transferência não limpa (conteúdo não textual)."

#: applet.js:176
#, javascript-format
msgid "An error occurred while adding content to clipboard history: %s"
msgstr "Ocorreu um erro ao adicionar conteúdo ao histórico da área de transferência: %s"

#: applet.js:206
#, javascript-format
msgid "New content detected: “%s”"
msgstr "Novo conteúdo detectado: “%s”"

#: applet.js:210
#, javascript-format
msgid "The contents of the clipboard have not changed since last %s seconds."
msgstr "O conteúdo da área de transferência não mudou nos últimos %s segundos."

#: applet.js:215
msgid "Starting clipboard checking."
msgstr "Iniciando verificação da área de transferência."

#: clipboard-history@axaul->metadata.json->name
msgid "Clipboard history"
msgstr "Histórico da área de transferência"

#: clipboard-history@axaul->metadata.json->description
msgid "Utility to access previously copied text."
msgstr "Utilitário para acessar textos copiados anteriormente."

#: clipboard-history@axaul->settings-schema.json->debug_mode->description
msgid "Enable debug mode"
msgstr "Ativar modo de depuração"

#: clipboard-history@axaul->settings-schema.json->clipboard_history_limit->description
msgid "Maximum number of items that can be stored in the clipboard history."
msgstr "Número máximo de itens que podem ser armazenados no histórico da área de transferência."

#: clipboard-history@axaul->settings-schema.json->poll_interval->description
msgid "Clipboard check interval"
msgstr "Intervalo de verificação da área de transferência"

#: clipboard-history@axaul->settings-schema.json->poll_interval->units
msgid "seconds"
msgstr "segundos"

#: clipboard-history@axaul->settings-schema.json->open_applet_shortcut->description
msgid "Open clipboard history with shortcut."
msgstr "Abrir histórico da área de transferência com atalho."

#: clipboard-history@axaul->settings-schema.json->open_applet_shortcut->tooltip
msgid "Save keyboard shortcuts to open the clipboard history."
msgstr "Salvar atalhos de teclado para abrir o histórico da área de transferência."
